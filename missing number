#include <iostream>

using namespace std;

int main()
{
    long long int n,arr_sum=0,sum,miss;
    cin>>n;                  //input n value
    long long int arr[n];
    for(int i=0;i<n-1;i++)
    {
        cin>>arr[i];         //input array element
        arr_sum+=arr[i];     //sum of array elements
    }
    sum=n*(n+1)/2;           //sum of n numbers
    miss=sum-arr_sum;        //to find missing number
    cout<<miss;
    
    return 0;
}
